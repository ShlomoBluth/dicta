{
  "name": "dictahomepagetest",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "cy:run": "npm run runChrome & npm run runFirefox",
    "cy:runSpec": "npm run runChrome -- --spec %npm_config_spec% & npm run runFirefox -- --spec %npm_config_spec%",
    "runChrome": "cypress run --browser chrome --reporter mochawesome --reporter mochawesome --reporter-options \"reportFilename=chrome,reportDir=cypress/report/mochawesome-report,overwrite=false,timestamp=mmddyyyy_HHMMss,json=false,html=true\"",
    "runFirefox": "cypress run --browser firefox --reporter mochawesome --reporter mochawesome --reporter-options \"reportFilename=firefox,reportDir=cypress/report/mochawesome-report,overwrite=false,timestamp=mmddyyyy_HHMMss,json=false,html=true\""
  },
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "cypress": "^6.1.0",
    "cypress-wait-until": "^1.7.1",
    "mocha": "^8.2.1",
    "mochawesome": "^6.2.1",
    "mochawesome-merge": "^4.2.0",
    "npm-run-all": "^4.1.5",
    "npx-run": "^2.1.2"
  }
}
